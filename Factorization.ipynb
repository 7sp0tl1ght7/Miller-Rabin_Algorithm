    "n = 3215031751\n",
    "for k in range(2,100):\n",
    "    D = pow(k-1,2) + 4*k*n\n",
    "    print('D=',D)\n",
    "    s = math.sqrt(D)\n",
    "    print('s=',s)\n",
    "    if s % 1 == 0:\n",
    "        x = int(((k-1)*(-1) + s) / (2*k))\n",
    "        print('x=',x)\n",
    "        break\n",
    "p = x + 1\n",
    "q = k*x + 1\n",
    "print('p=',p,'q=',q)"
